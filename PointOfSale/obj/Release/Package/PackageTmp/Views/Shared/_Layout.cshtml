<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.title -Point Of Sale</title>     

    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <link href="~/font-awesome/css/font-awesome.css" rel="stylesheet" />
    <link href="~/Content/plugins/iCheck/custom.css" rel="stylesheet" />
    <link href="~/Content/plugins/dataTables/datatables.min.css" rel="stylesheet" />
    <link href="~/Content/animate.css" rel="stylesheet" />
    <link href="~/Content/style.css" rel="stylesheet" />
    <!-- Chosen DDL -->
    <link href="~/Content/plugins/SweatAlert2/sweetalert2.min.css" rel="stylesheet" />
    <link href="~/Content/plugins/chosen/bootstrap-chosen.css" rel="stylesheet" />
    @*<link href="~/Content/plugins/sweetalert/sweetalert.css" rel="stylesheet" />*@
    <!--Summernote-->
    <link href="~/Content/plugins/summernote/summernote-bs3.css" rel="stylesheet" />
    <link href="~/Content/plugins/summernote/summernote.css" rel="stylesheet" />
    <!--Dropzone-->
    <link href="~/Content/plugins/dropzone/basic.css" rel="stylesheet" />
    <link href="~/Content/plugins/dropzone/dropzone.css" rel="stylesheet" />
    <!--Jasny-->
    <link href="~/Content/plugins/jasny/jasny-bootstrap.min.css" rel="stylesheet" />
    <link href="~/Plugins/appendGrid/jquery-ui.structure.min.css" rel="stylesheet" />
    <link href="~/Plugins/appendGrid/jquery-ui.theme.min.css" rel="stylesheet" />
    <link href="~/Plugins/appendGrid/jquery.appendGrid-1.6.2.min.css" rel="stylesheet" />
    @*<script src="~/Scripts/jquery-3.1.1.min.js"></script>*@

    <link href="~/Content/plugins/datapicker/datepicker3.css" rel="stylesheet" />
    <link href="~/Content/WXCustomStyles.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-2.1.1.js"></script>
</head>
<body>
    <div id="wrapper">
        <!--Left Navigation-->
        @Html.Partial("_LeftNav")
        <div id="page-wrapper" class="gray-bg">
            @Html.Partial("_TopNav")
            <div class="wrapper wrapper-content">
                @RenderBody()
                <div style="margin-bottom:35px"></div>
            </div>
            @Html.Partial("_Footer")
        </div>
    </div>

    <!-- Mainly scripts -->

    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/Scripts/plugins/metisMenu/jquery.metisMenu.js"></script>
    <script src="~/Scripts/plugins/slimscroll/jquery.slimscroll.min.js"></script>
    <!-- i Check -->
    <script src="~/Scripts/plugins/iCheck/icheck.min.js"></script>
    <script>
        $(document).ready(function () {
            $('.i-checks').iCheck({
                checkboxClass: 'icheckbox_square-green',
                radioClass: 'iradio_square-green',
            });
        });
    </script>
    <!-- Flot -->
    @*<script src="~/Scripts/plugins/flot/jquery.flot.js"></script>
        <script src="~/Scripts/plugins/flot/jquery.flot.tooltip.min.js"></script>
        <script src="~/Scripts/plugins/flot/jquery.flot.spline.js"></script>
        <script src="~/Scripts/plugins/flot/jquery.flot.resize.js"></script>
        <script src="~/Scripts/plugins/flot/jquery.flot.pie.js"></script>
        <script src="~/Scripts/plugins/flot/jquery.flot.symbol.js"></script>
        <script src="~/Scripts/plugins/flot/jquery.flot.time.js"></script>*@

    <!-- Peity -->
    @*<script src="~/Scripts/plugins/peity/jquery.peity.min.js"></script>
        <script src="~/Scripts/demo/peity-demo.js"></script>*@

    <!-- Custom and plugin javascript -->
    <script src="~/Scripts/inspinia.js"></script>
    <script src="~/Scripts/plugins/pace/pace.min.js"></script>

    <!-- jQuery UI -->
    <script src="~/Scripts/plugins/jquery-ui/jquery-ui.min.js"></script>

    <!-- Jvectormap -->
    @*<script src="~/Scripts/plugins/jvectormap/jquery-jvectormap-2.0.2.min.js"></script>
        <script src="~/Scripts/plugins/jvectormap/jquery-jvectormap-world-mill-en.js"></script>*@

    <!-- EayPIE -->
    @*<script src="~/Scripts/plugins/easypiechart/jquery.easypiechart.js"></script>*@

    <script src="~/Scripts/datepicker/daterangepicker.js"></script>

    <!-- Sparkline -->
    <script src="~/Scripts/plugins/sparkline/jquery.sparkline.min.js"></script>

    <!-- Sparkline demo data  -->
    @*<script src="~/Scripts/demo/sparkline-demo.js"></script>*@
    
    <!-- Data Tables -->
    <script src="~/Scripts/plugins/dataTables/datatables.min.js"></script>

    <!-- Validate -->
    <script src="~/Scripts/plugins/validate/jquery.validate.min.js"></script>

    <!-- Chozen plugin -->
    <script src="~/Scripts/plugins/chosen/chosen.jquery.js"></script>

    <!-- Sweat Alert -->
    <script src="~/Content/plugins/SweatAlert2/sweetalert2.min.js"></script>
    @*<script src="~/Scripts/plugins/sweetalert/sweetalert.min.js"></script>*@

    <!--Summernote-->
    @*<script src="~/Scripts/plugins/summernote/summernote.min.js"></script>*@

    <!--Dropzone-->
    <script src="~/Scripts/plugins/dropzone/dropzone.js"></script>
    <!--Jasny-->
    <script src="~/Scripts/plugins/jasny/jasny-bootstrap.min.js"></script>

    <script src="~/Plugins/appendGrid/jquery-ui-1.11.1.min.js"></script>
    <script src="~/Plugins/appendGrid/jquery.appendGrid-1.6.2.min.js"></script>

    <script src="~/Scripts/plugins/datapicker/bootstrap-datepicker.js"></script>

    

</body>
</html>
