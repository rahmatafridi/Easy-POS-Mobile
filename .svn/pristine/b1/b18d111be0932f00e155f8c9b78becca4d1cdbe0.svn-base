
@{
    ViewBag.Title = "StockIndex";
}

<div class="row">
    <div class="col-lg-12 col-md-12 col-sm-12">
        <div class="ibox">
            <div class="ibox-heading">
                <div class="ibox-title">
                    <h2><i class="fa fa-cogs">&nbsp;Stock InHand Report</i></h2>
                </div>
            </div>
            <div class="ibox ibox-content">
                <table class="table table-striped table-bordered" id="tblStockReport">
                    <thead>
                        <tr>
                            <th>Item</th>
                            <th>Barcode</th>
                            <th>Category</th>
                            <th>Quantity</th>

                        </tr>
                    </thead>
                    <tbody id="tbody"></tbody>
                </table>
            </div>
        </div>
    </div>
</div>

<script src="~/Assets/PagesJS/StockReport.js"></script>