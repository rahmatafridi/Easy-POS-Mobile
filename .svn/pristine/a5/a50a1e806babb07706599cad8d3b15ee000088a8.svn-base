
@{
    ViewBag.Title = "TransferIndex";
}

<div class="row">
    <div class="col-lg-12 col-md-12 col-sm-12">
        <div class="ibox">
            <div class="ibox-heading">
                <div class="ibox-title">
                    <h2><i class="fa fa-cogs">&nbsp;Transfer / Issuance Report</i></h2>
                </div>
            </div>
            <div class="ibox ibox-content">
                <table class="table table-striped table-bordered" id="tblTrReport">
                    <thead>
                        <tr>
                            <th class="hidden">UID</th>
                            <th>Transfer#</th>
                            <th>Location</th>
                            <th>Date</th>
                            <th></th>
                        </tr>
                    </thead>
                    <tbody id="tbody"></tbody>
                </table>
            </div>
        </div>
    </div>
</div>
<script src="~/Assets/PagesJS/TransferReport.js"></script>