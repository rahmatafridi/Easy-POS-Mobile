
@{
    ViewBag.Title = "VendorIndex";
}


<div class="row hidden-print">
    <div class="col-lg-12 col-md-12 col-sm-12">
        <div class="ibox">
            <div class="ibox-heading">
                <div class="ibox-title">
                    <h2><i class="fa fa-cogs">&nbsp;Vendor's Report</i></h2>
                </div>
            </div>
            <div class="ibox ibox-content">
                <div class="row hidden-print ">
                    <div class="col-lg-5 col-md-5 col-lg-offset-1 col-md-offset-1">
                        <div class="form-group" id="dateFrom">
                            <label class="font-normal">From Date<span class="required" style="color:red"></span></label>
                            <div class="input-group date">
                                <span class="input-group-addon"><i class="fa fa-calendar"></i></span><input type="text" class="form-control" id="fDate">
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-5 col-md-5">
                        <div class="form-group" id="dateTo">
                            <label class="font-normal">To Date<span class="required" style="color:red"></span></label>
                            <div class="input-group date">
                                <span class="input-group-addon"><i class="fa fa-calendar"></i></span><input type="text" class="form-control" id="tDate">
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


<div class="row">
    <div class="col-lg-12 col-md-12 col-sm-12">
        <div class="ibox">
            <div class="ibox-heading">
                <div class="ibox-title">
                    <h2><i class="fa fa-cogs">&nbsp;Vendor's Report</i></h2>
                </div>
            </div>
            <div class="ibox ibox-content">
                <table class="table table-striped table-bordered" id="tblVendorReport">
                    <thead>
                        <tr>
                            <th class="hidden">UID</th>
                            <th>Name</th>
                            <th>Contact</th>
                            <th>Address</th>
                            <th>Current Balance</th>
                            <th></th>
                            <th></th>
                        </tr>
                    </thead>
                    <tbody id="tbody"></tbody>
                </table>
            </div>
        </div>
    </div>
</div>

<script src="~/Assets/PagesJS/VendorReport.js"></script>